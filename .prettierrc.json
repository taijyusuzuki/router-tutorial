  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",

  "overrides": [
    {
      "files": ["*.html", "*.ejs"],
      "options": {
        "printWidth": 200
      }
    }
  ]
}
